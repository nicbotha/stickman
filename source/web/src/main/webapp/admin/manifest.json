{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "stickman.adminapp",
		"type": "application",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"svcRemote": {
				"uri": "/web/odata",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "stickman.adminapp.view.App",
			"type": "XML",
			"id": "app"
		},
		"handleValidation": true,
		"autoPrefixId": true,
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.38.0"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "stickman.adminapp.i18n.i18n"
				}
			},
			"": {
				"dataSource": "svcRemote"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "stickman.adminapp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "appHome",
					"target": "home"
				},
				{
					"pattern": "some",
					"name": "someList",
					"target": "someList"
				},
				{
					"pattern": "some/{id}",
					"name": "some",
					"target": "some"
				},
				{
					"pattern": "somecreate",
					"name": "someCreate",
					"target": "someCreate"
				},
				{
					"pattern": "fileresourcelist",
					"name": "fileresourceList",
					"target": "fileresourceList"
				},
				{
					"pattern": "fileresource/{id}",
					"name": "fileresourceView",
					"target": "fileresourceView"
				},
				{
					"pattern": "fileresourcecreate",
					"name": "fileresourceCreate",
					"target": "fileresourceCreate"
				}			
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				},
				"someList": {
					"viewPath": "stickman.adminapp.view.some",
					"viewName": "SomeList",
					"viewLevel": 2
				},
				"some": {
					"viewName": "some.Some",
					"viewLevel": 3
				},
				"someCreate": {
					"viewName": "some.SomeCreate",
					"viewLevel": 3,
					"transition": "flip"
				},
				"fileresourceList": {
					"viewPath": "stickman.adminapp.view.fileresource",
					"viewName": "FileResourceList",
					"viewLevel": 2
				},
				"fileresourceView": {
					"viewName": "fileresource.FileResourceView",
					"viewLevel": 3
				},
				"fileresourceCreate": {
					"viewName": "fileresource.FileResourceCreate",
					"viewLevel": 3,
					"transition": "flip"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	}
}
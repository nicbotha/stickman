<mvc:View
	controllerName="stickman.adminapp.controller.some.SomeCreate"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:core="sap.ui.core"
	busyIndicatorDelay="0">
	<Page
		id="somePage"
		title="{i18n>Some.create.title}"
		showNavButton="true"
		navButtonPress="onNavBack"
		class="sapUiResponsiveContentPadding">
		<core:Fragment fragmentName="stickman.adminapp.view.some.SomeEdit" type="XML"/>
		<footer>
			<Bar>
				<contentRight>
					<Button id="save" text="{i18n>Label.Save}" type="Emphasized" visible="true" press="onSavePress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>

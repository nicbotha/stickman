<mvc:View
	controllerName="stickman.adminapp.controller.fileresource.FileResourceView"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:core="sap.ui.core"
	busyIndicatorDelay="0">
	<Page
		id="page"
		title="{i18n>Page.Tile.FileResouce.view} {Name}"
		showNavButton="true"
		navButtonPress="onNavBack"
		class="sapUiResponsiveContentPadding">		
		<footer>
			<Bar>
				<contentRight>
					<Button id="edit" text="{i18n>Label.Edit}" type="Emphasized" visible="true" press="onEditPress" />
					<Button id="delete" text="{i18n>Label.Delete}" type="Emphasized" visible="true" press="onDeletePress" />
					<Button id="save" text="{i18n>Label.Save}" type="Emphasized" visible="false" press="onSavePress" />
					<Button id="cancel" text="{i18n>Label.Cancel}" type="Emphasized" visible="false" press="onCancelPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>

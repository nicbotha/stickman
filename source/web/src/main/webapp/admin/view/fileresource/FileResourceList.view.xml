<mvc:View controllerName="stickman.adminapp.controller.fileresource.FileResourceList" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">

	<Page title="{i18n>Page.Tile.FileResouce}" class="sapUiResponsiveContentPadding" showNavButton="true" navButtonPress="onNavBack">
		<content>
			 <List id="productList"
		        items="{path: '/value'}">
		        <headerToolbar>
					<Toolbar>
						<Title text="{i18n>View.Title.FileResource}"></Title>
					</Toolbar>
				</headerToolbar>
                <ObjectListItem
                	intro="{ID}"
                    title="{Name}"
                    type="Navigation"
                    press=".onListItemPressed">
                </ObjectListItem>
		    </List>
		</content>
		<footer>
			<OverflowToolbar id="footer">
				<ToolbarSpacer />
				<OverflowToolbarButton text="Add" icon="sap-icon://add" press="onCreate"/>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>